{
  "name": "spreadsheet-code-generator",
  "version": "0.0.2",
  "description": "now construction",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:tsc": "npm run tsc",
    "build:scss": "node-sass --output-style compressed --source-map true --source-map-contents true src/scss/style.scss -o dist/css",
    "build:font-awesome-fonts": "cpx -C \"node_modules/font-awesome/fonts/*\" \"dist/fonts\"",
    "build:jade-index": "npm run jade-index",
    "build:webpack": "npm run webpack-production",
    "electron": "electron",
    "electron-packager": "electron-packager",
    "electron-packager-dist": "electron-packager . spreadsheet-code-generator --platform=darwin,win32 --arch=x64 --overwrite --ignore=node_modules",
    "jade-index": "jade index.jade --pretty",
    "ncu": "ncu",
    "npm-check-updates": "npm-check-updates",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "webpack": "webpack --watch --config webpack.config.js",
    "webpack-production": "cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://nashika@github.com/nashika/spreadsheet-code-generator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nashika/spreadsheet-code-generator/issues"
  },
  "homepage": "https://github.com/nashika/spreadsheet-code-generator#readme",
  "devDependencies": {
    "@types/ace": "^0.0.35",
    "@types/handsontable": "^0.31.0",
    "@types/lodash": "^4.0.0",
    "@types/loglevel": "^1.0.0",
    "@types/node": "^8.0.0",
    "@types/webpack": "^3.0.0",
    "ace-builds": "^1.0.0",
    "awesome-typescript-loader": "^3.0.0",
    "bootstrap": "^4.0.0-beta",
    "bootstrap-sass": "^3.0.0",
    "bootstrap-vue": "^1.0.0-beta.9",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "electron": "^1.0.0",
    "electron-packager": "^9.0.0",
    "file-loader": "^1.0.0",
    "font-awesome": "^4.0.0",
    "handsontable": "^0.34.0",
    "html-loader": "^0.5.0",
    "lodash": "^4.0.0",
    "loglevel": "^1.0.0",
    "node-sass": "^4.0.0",
    "npm-check-updates": "^2.0.0",
    "npm-run-all": "^4.0.0",
    "pug": "^2.0.0-rc.0",
    "pug-html-loader": "^1.0.0",
    "raw-loader": "^0.5.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.19.0",
    "typescript": "^2.0.0",
    "url-loader": "^0.6.0",
    "vue": "^2.0.0",
    "vue-class-component": "^6.0.0",
    "vue-loader": "^13.3.0",
    "webpack": "^3.0.0"
  }
}
